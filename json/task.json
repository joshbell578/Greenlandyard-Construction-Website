{
    "version": "0.1.0",
    "command": "node",
    "isShellCommand": true,
    "args": [
        "--harmony"
    ],

    "tasks": [
        {
            "taskName": "runFile",
            "suppressTaskName": true,
            "showOutput": "always",
            "problemMatcher": "$jshint",
            "args": ["${file}"]
        }

        ,{
            "taskName": "getResults",
            "suppressTaskName": true,
            "showOutput": "always"
        }

        ,{
            "taskName": "conFig",
            "suppressTaskName": true,
            "showOutput": "always"
        }
    ]
    
}